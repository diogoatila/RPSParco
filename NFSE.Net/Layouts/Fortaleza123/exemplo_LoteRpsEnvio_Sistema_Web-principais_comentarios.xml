            <ns2:RegimeEspecialTributacao>1</ns2:RegimeEspecialTributacao>
            <ns2:OptanteSimplesNacional>2</ns2:OptanteSimplesNacional> <!-- Valor 1 para optante e 2 para não optante do simples nacional -->
